<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test name="Contact Page Regression Test">
		<groups>
			<run>
				<include name="LoginPage"></include>
				<include name="HomePage"></include>
				<include name="ContactPage"></include>
			</run>
		</groups>
		<classes>
			<class name="TestLayer.LoginPage" />
			<class name="TestLayer.HomePage" />
			<class name="TestLayer.ContactPage" />
		</classes>
	</test>

	<test name="Companies Page Test cases">
		<groups>
			<run>
				<include name="LoginPage"></include>
				<include name="HomePage"></include>
				<include name="CompanyPage"></include>
			</run>
		</groups>
		<classes>
			<class name="TestLayer.LoginPage" />
			<class name="TestLayer.HomePage" />
			<class name="TestLayer.CompaniesPage"></class>
		</classes>
	</test>

</suite> 